{
  "version": 2,
  "builds": [
    {
      "src": "index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "ipx/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "tmdb/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/ipx(.*)",
      "dest": "/ipx/index.ts"
    },
    {
      "src": "/tmdb(.*)",
      "dest": "/tmdb/index.ts"
    },
    {
      "src": "/",
      "dest": "/index.ts"
    }
  ]
}
